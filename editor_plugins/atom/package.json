{
  "name": "orlang",
  "main": "./lib/orlang",
  "version": "0.0.0",
  "description": "Atom plugin for the orlang",
  "keywords": [
  ],
  "repository": "https://github.com/orktes/orlang",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "^9.0.0",
    "atom-package-deps": "^4.0.1"
  },
  "configSchema": {
    "executablePath": {
      "title": "orlang Executable Path",
      "description": "The path to `orlang` executable",
      "type": "string",
      "default": "orlang"
    }
  },
  "package-deps": ["linter"],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
